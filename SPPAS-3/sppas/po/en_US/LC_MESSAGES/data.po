msgid ":ERROR 2000: "
msgstr "No audio file defined."

msgid ":ERROR 2005: "
msgstr "Audio type error: not supported file format {extension}. "

msgid ":ERROR 2010: "
msgstr "{filename} opening, reading or writing error: {message}."

msgid ":ERROR 2015: "
msgstr "No data or corrupted data in the audio file {filename}."

msgid ":ERROR 2020: "
msgstr "{number} is not a right index of channel."

msgid ":ERROR 2025: "
msgstr "From {value1} to {value2} is not a proper interval."

msgid ":ERROR 2050: "
msgstr "No channel defined."

msgid ":ERROR 2060: "
msgstr "Channels have not the same sample width."

msgid ":ERROR 2061: "
msgstr "Channels have not the same frame rate."

msgid ":ERROR 2062: "
msgstr "Channels have not the same number of frames."

msgid ":ERROR 2070: "
msgstr "Invalid sample width. Expect 1, 2 or 4. Got {value}."

msgid ":ERROR 2080: "
msgstr "Invalid framerate. Got {value}."

msgid ":ERROR 2610: "
msgstr "Image of file {filename} can't be read by opencv library."

msgid ":ERROR 2620: "
msgstr "Image of file {filename} can't be written by opencv library."

msgid ":ERROR 2330: "
msgstr "The value {} can't be superior to the one of the image {}."

msgid ":ERROR 2332: "
msgstr "The width value {} can't be superior to the width {} of the image."

msgid ":ERROR 2334: "
msgstr "The height value {} can't be superior to the height {} of the image."

msgid ":ERROR 2336: "
msgstr "The x-axis value {} can't be superior to the width {} of the image."

msgid ":ERROR 2338: "
msgstr "The y-axis value {} can't be superior to the height {} of the image."

msgid ":ERROR 3610: "
msgstr "Video of file {filename} can't be opened by opencv library."

msgid ":ERROR 3620: "
msgstr "Video of file {filename} can't be written by opencv library."

msgid ":ERROR 3630: "
msgstr "Video of file {filename} can't be read by OpenCV library."

msgid ":ERROR 3640: "
msgstr "A video has already locked the video stream."
